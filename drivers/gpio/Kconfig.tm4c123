	depends on GPIO_TM4C123_F2
	default "GPIO_F2"

config GPIO_TM4C123_F2_IRQ_PRI
	int "GPIO F2 interrupt priority"
	depends on GPIO_TM4C123_F2
	range 0 5
	default 1
	help
	  TM4C123 GPIO F2 IRQ priority.

